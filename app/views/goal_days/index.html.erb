<p id="notice"><%= notice %></p>

<h1>Goal Days</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_days.each do |goal_day| %>
      <tr>
        <td><%= link_to 'Show', goal_day %></td>
        <td><%= link_to 'Edit', edit_goal_day_path(goal_day) %></td>
        <td><%= link_to 'Destroy', goal_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal Day', new_goal_day_path %>
