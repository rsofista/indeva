<p id="notice"><%= notice %></p>

<h1>Seller Goals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seller_goals.each do |seller_goal| %>
      <tr>
        <td><%= link_to 'Show', seller_goal %></td>
        <td><%= link_to 'Edit', edit_seller_goal_path(seller_goal) %></td>
        <td><%= link_to 'Destroy', seller_goal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller Goal', new_seller_goal_path %>
