<p id="notice"><%= notice %></p>

<h1>Store Sellers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_sellers.each do |store_seller| %>
      <tr>
        <td><%= link_to 'Show', store_seller %></td>
        <td><%= link_to 'Edit', edit_store_seller_path(store_seller) %></td>
        <td><%= link_to 'Destroy', store_seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store Seller', new_store_seller_path %>
